          "_customHTML": "<section>\n\n<mbr-parameters>\n<!-- Block parameters controls (Blue \"Gear\" panel) -->\n<input type=\"range\" inline=\"\" title=\"Social Icons\" name=\"socialIcons\" min=\"0\" max=\"6\" step=\"1\" value=\"2\">\n<input type=\"range\" inline=\"\" title=\"Border Radius\" name=\"borderRadius\" min=\"0\" max=\"50\" step=\"10\" value=\"0\">\n<input type=\"range\" inline=\"\" title=\"Padding\" name=\"padding\" min=\"0\" max=\"2\" step=\"0.1\" value=\"1\">\n<input type=\"range\" inline=\"\" title=\"Font Size\" name=\"fontSize\" min=\"1\" max=\"4\" step=\"0.2\" value=\"1\">\n<input type=\"range\" inline=\"\" title=\"Icon Spacing\" name=\"iconSpacing\" min=\"0\" max=\"2\" step=\"0.1\" value=\"0\">\n<input type=\"color\" title=\"Icon Color\" name=\"iconColor\" value=\"#ffffff\">\n<input type=\"color\" title=\"Hover Color\" name=\"hoverColor\" value=\"#000000\">\n<input type=\"color\" title=\"Background Color Icon 1\" name=\"bgColorIcon1\" value=\"#3B5998\">\n<input type=\"color\" title=\"Background Color Icon 2\" name=\"bgColorIcon2\" value=\"#ff3366\">\n<input type=\"color\" title=\"Background Color Icon 3\" name=\"bgColorIcon3\" value=\"#dd4b39\">\n<input type=\"color\" title=\"Background Color Icon 4\" name=\"bgColorIcon4\" value=\"#007bb5\">\n<input type=\"color\" title=\"Background Color Icon 5\" name=\"bgColorIcon5\" value=\"#bb0000\">\n<input type=\"color\" title=\"Background Color Icon 6\" name=\"bgColorIcon6\" value=\"#232323\">\n<!-- End block parameters -->\n</mbr-parameters>\n\n<div class=\"witsec-socialbar\">\n<a href=\"https://www.facebook.com\" class=\"icon icon1\"><span mbr-icon=\"\" class=\"socicon socicon-facebook mbr-iconfont mbr-iconfont-btn\"></span></a>\n<a href=\"https://twitter.com\" mbr-if=\"socialIcons > 1\" class=\"icon icon2\"><span mbr-icon=\"\" class=\"mbr-iconfont mbr-iconfont-btn socicon-instagram socicon\"></span></a>\n<a href=\"https://www.google.com\" mbr-if=\"socialIcons > 2\" class=\"icon icon3\"><span mbr-icon=\"